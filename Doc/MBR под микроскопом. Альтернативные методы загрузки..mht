html,body {font-family:arial, sans-serif;font-size:13px;margin:0;padding:0;}
body {background-color:white;}

a {text-decoration:none;color:#285A99;}
a:hover {text-decoration:underline;}

.content .rawcode {background:#e0e0e0;padding:10px;}
.content .rawsource {background:#e0e0e0;float:right;color:#ccc;padding:10px;}
.content #cn_comments .pre {margin-bottom:0;}
.content #cn_comments .quotetitle {font-size:10px;}
.content #cn_comments .qotetext {font-size:10px;}
.content #cn_comments .quotetitle {margin-left:30px;font-size:10px;}
.content #cn_comments .qotetext {color:#888;background-color:#f8f8f8;border:solid 1px #eee;padding:10px;margin:0 0 0 30px;}
.content #cn_comments .mp0 {margin:0;padding:0;}


.content li {font-size:13px;color:#404040;margin-top:5px;}
.content h1 {font-size:20px;color:#606060;}
.content h2 {font-size:16px;color:#606060;}
.content h3 {font-size:14px;color:#606060;margin:0;padding:0;margin-bottom:10px;}
.content h4 {font-size:13px;background:#f2f9fc;border-bottom:solid 1px #7B94C3;padding:10px;}
.content p {color:#404040;text-align:left;}
.content td,th {padding:6px;}
.content .gray {color:#808080;}

.t0 {background-color:#c6d6de;}
.t2 {background-color:#f2f9fc;}

.hline {padding:0;margin:0;width:100%;border-bottom:solid 1px #C6D6DE;}

.content .m0 {margin:0;padding:0;}

.comment {color:#a0a0a0;}

.content pre {
	color:#0000bb;
	font-size:12px;
	border-left:2px dashed #e0e0e0;
	padding:10px 20px 10px 10px;
	margin: 0 0 0 10px;
	font-family: courier new, courier;
	}

.content .pre {
	color:#0000bb;
	font-size:12px;
	border:1px solid #e0e0e0;
	background: #f8f8f8;
	padding:10px 20px 10px 10px;
	margin: 0 0 20px 0;
	font-family: courier new, courier;
	overflow:hidden;
	}

.rss{background-color:darkorange;width:22px;font-weight:bold;font-size:10px;padding:3px;margin:5px 5px 0 0;float:left;}
.rss>a{color:white}

.twitter {background-color:#4ad2ff;width:42px;font-weight:bold;font-size:10px;padding:3px;margin:5px 5px 0 0;float:left;}
.twitter>a {color:white;}

.hint {font-size:11px;color:#888;}

.colortable {border-collapse:collapse;}
.colortable tr>th {background: #ddd;padding:6px;border:1px solid white;}
.colortable tr>td {background: #f8f8f8;padding:6px;border:1px solid white;}
.colortable tr:nth-child(even)>td {background: #f0f0f0;}



























#cnwrapper {padding:10px;min-width:240px;max-width:1280px;margin:0 auto; background: white;}
#cnfooter {padding:10px;border-top:solid 1px #C6D6DE;text-align:center;background: white;max-width:1280px;margin:10px auto;}

@media screen and (min-width : 1280px) {
	body {background: #f7f9fa;}
	}
	
a img {border:0;}

.leftcol {float:left;width:20%;}
.centercol {width:79%;float:left;margin-left:1%;}

.leftcol form,
.leftcol table,
.leftcol tr,
.leftcol td,
.leftcol th,
.leftcol div {color:#000000;margin:0;padding:0;}

.leftcol .se {border-bottom: 1px solid #7B94C3;padding:10px;background-color:#c6d6de;overflow:hidden;margin:0 0 10px 0;}
.leftcol .se ul {padding:5px 0 5px 10px;margin:0;}
.leftcol .se {line-height: 120%;}
.leftcol .se h3 {font-size:14px;color:#333;margin:0;padding:0;margin-bottom:10px;font-weight:bold;line-height: 120%;}

.leftcol .se .hint {font-size:10px;color:#808080;}
.leftcol .se .hint a {font-size:10px;}

.leftcol .se #flt ul {padding:0;margin:0;list-style:none;}
.leftcol .se #flt li {padding:0;margin:0 0 5px 0;list-style:none;font-size:11px;color:#666;}
.leftcol .se #flt li .form_theme_title {font-size:13px;}

@media screen and (max-width : 850px) {
	.leftcol input[type=text], input[type=submit], input[type=button] {font-size:11px;padding:2px;}
	.leftcol .se h3 {font-size:12px;}
	.leftcol {font-size:11px;}
	.leftcol .se #flt li {font-size:10px;}
	.leftcol .se #flt li .form_theme_title {font-size:11px;}
	}

ul.error {border:1px solid red;padding:5px 10px 10px 30px;background-color:#fff8f8;}
ul.error li {color:red;}

/* Articles cat */
.dsc {padding:10px 0 5px 10px;color:gray;}
.ath {padding-left:10px;padding-top:5px;padding-bottom:20px;}
.cat {font-size:15px;font-weight:bold;}
div.url {padding-left:10px;}
a.url {color:green;}

div.hr {border-bottom:1px solid #c6d6de;height:1px;}

.catalog a {font-weight:bold;}
.catalog ul {list-style:none;}
.catalog ul li {margin-bottom:5px;}
.catalog ul li a {margin-bottom:5px;}

/* Head */
#cnheader #logo {float:left;width:20%;}
#cnheader #logo div {margin:30px 0 0 30px;}
#cnheader #searchform {width:79%;margin:0 0 0 21%;background:#c6d6de;border-bottom:1px solid #7b94c3;}
#cnheader #searchform #search1 {width:70%;float:left;}
#cnheader #searchform #search2 {width:30%;margin-left:70%}

#cnheader #searchform #search1 table {width:100%;}
#cnheader #searchform #search1 table td {width:100%;padding:22px 10px 5px 10px;}
#cnheader #searchform #search1 table .sft1 {width:100%;}
#cnheader #searchform #search1 input[type=text] {width:100%;}
#cnheader #searchform #search1 ul {list-style:none;padding:0;margin:0;margin:3px 0 0 50px;}
#cnheader #searchform #search1 li {list-style:none;padding:0;margin:0 0 0 3px;float:left;}
#cnheader #searchform #search1 a {display:inline-block;padding:7px 12px 7px 12px;border-top:1px solid white;border-left:1px solid white;font-size:12px;margin-bottom:-1px;}
#cnheader #searchform #search1 li.active a {background:white;border-top:1px solid #7b94c3;border-left:1px solid #7b94c3;border-right:1px solid #7b94c3;border-bottom:1px solid white;}
#cnheader #searchform #search1 li.activedark a {background:#e5eef2;border-top:1px solid #7b94c3;border-left:1px solid #7b94c3;border-right:1px solid #7b94c3;border-bottom:1px solid #e5eef2;}

#cnheader #searchform #search2 div.t {font-size:12px;padding:5px 0 1px 2px;}
#cnheader #searchform #search2 table {width:100%;}
#cnheader #searchform #search2 table td.sft1 {width:100%;padding-right:15px;}
#cnheader #searchform #search2 table td.sft2 {width:100%;padding-right:10px;}
#cnheader #searchform #search2 input[type=text] {width:100%;}
#cnheader #searchform #search2 select {margin-left:3px;}

/* Main */
#cn_main #c1 {width:33%;float:left;margin-left:1%;}
#cn_main #c2 {width:32%;float:left;margin-left:1%;}
#cn_main #c3 {width:32%;float:left;margin-left:1%;}

@media screen and (max-width : 1000px) {
	#cn_main #c1 {width:49%;float:left;margin-left:0;}
	#cn_main #c2 {width:49%;float:right;margin:0 1% 0 0;}
	#cn_main #c3 {width:49%;float:left;margin:0;}
	#cn_main #c3_vk {display:none;}
	}

@media screen and (max-width : 950px) {
	#cnheader #searchform #search1 {width:100%;}
	#cnheader #searchform #search2 {display:none;}	
	}

@media screen and (max-width : 710px) {
	.leftcol {float:none;width:auto;margin:0;clear:both;position:relative;}
	.centercol {float:none;width:auto;margin:0;clear:both;}
	.leftcol #np_block_1_2 {display:none;}
	.leftcol #np_block_1_3 {display:none;}
	.leftcol #np_block_1_4 {display:none;}
	.leftcol #np_block_1_5 {display:none;}
	.leftcol #np_block_1_6 {display:none;}
	.leftcol #np_block_1_7 {display:none;}
	.btop {display:none;}
	}

@media screen and (max-width : 650px) {
	#cnheader {padding:0;margin:0;}
	#cnheader #searchform {width:auto;margin:0;}
	#cnheader #logo {float:none;width:auto;}
	#cnheader #logo div {margin:1%;}
	}

@media screen and (max-width : 559px) {
	#cnheader #searchform #search1 li a {padding:5px;}
	#cnheader #searchform #search1 li.active a {padding:5px;}
	}

@media screen and (max-width : 481px) {
	#cnheader #searchform #search1 ul {margin:0;padding:0;}
	#cnheader #searchform #search1 li {list-style:none;padding:0;margin:0 3px;float:none;width:auto;border:none;}
	#cnheader #searchform #search1 li a {width:auto;border:1px solid white;margin:0 0 3px 0;display:block;}
	#cnheader #searchform #search1 li.active a {width:auto;border:1px solid #7B94C3;margin:0 0 3px 0;display:block;}
	}

@media screen and (max-width : 440px) {
	#cn_main #c1,
	#cn_main #c2,
	#cn_main #c3 {width:auto;float:none;margin:0;}
	}


#cn_main .g {color:#808080;}
