Content-Disposition: inline; filename=main.css
Content-Type: text/css; name=main.css
Content-Location: http://dsme.narod.ru/main.css
Content-Transfer-Encoding: 8bit

body {
	font-family: sans-serif;
	margin:4px;
	padding:0px;
	background:#ffffff
}
div.title {
	border-style: solid;
	border-width: 1px;
	border-color: #c0c0c0;
	height: 60px;
	background: #f0f0f0;
	margin: 4px;
	padding: 0px;
	overflow:hidden
}
h2.title {
	padding: 6px 12px 0px 0px;
	margin: 0px;
	font-family: Verdana;
	font-size: 24px;
	text-align: right
}
div.leftmenu {
	float:left;
	border-style: solid;
	border-width: 1px;
	border-color: #c0c0c0;
	background: #f0f0f0;
	font-family: Verdana;
	font-size: 13px;
	cursor: default;
	margin: 4px 16px 16px 4px;
	padding: 0px
}
div.main {
	margin: 16px;
	padding: 0px
}
div.langbar {
	border-style: solid;
	border-width: 0px 0px 1px 0px;
	border-color: #c0c0c0;
	text-decoration: none;
	text-align: center;
	margin: 0px;
	padding: 4px
}
div.leftmenu a{
	display: block;
	text-decoration: none;
	color: #202020;
	padding: 0px 8px;
}
div.langbar a {
	display: inline;
	text-decoration: none;
}
a.menuactive, a.menuactive2, a.langactive {
	background:#e0e0e0;
	cursor: default;
}
a.menulink, a.menulink2, a.langlink {
	cursor: hand;
}
a.menuactive2 {
	text-indent: 16px;
}
a.menulink2 {
	text-indent: 16px;
}
a.menulink:hover, a.menulink:focus, a.menulink2:hover, a.menulink2:focus, a.langlink:hover, a.langlink:focus {
	background: #e0e0f0
}
div.ban {
	clear: both;
	font-family: Verdana;
	font-size: 13px;
	border-style: solid;
	border-width: 1px;
	border-color: #c0c0c0;
	background: #f0f0f0;
	margin: 4px;
	padding: 0px;
	overflow: hidden;
	text-align: center;
}

h1         {margin:1em 0em}
h2         {margin:1em 0em}
h3         {margin:1em 0em}
h4         {margin:1em 0em}
h5         {margin:1em 0em}
h6         {margin:1em 0em}

li {
	list-style-position: inside;
}

div.sign   {
	text-align:right;
	font-style:italic;
}

pre.codefilename {
	background: #e0e0e0;
	color: #000000;
	padding: 2px;
	margin: 0px;
	border-color: #c0c0c0;
	border-style: solid;
	border-width: 1px 1px 0px 1px;
}
pre.code {
	background: #f0f0f0;
	color: #000000;
	padding: 2px;
	margin: 0px;
	border: #c0c0c0 1px solid
}
a.pers {
	text-decoration:none;
	font-style:italic
}

div.epigraph{width: 50%; font-style: italic; margin: 0px 0px 0px 50%}


div.p {
	padding: 0px 0px 1em 0px;
	text-align: justify;
}

